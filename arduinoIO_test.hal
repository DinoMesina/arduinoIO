loadusr -W arduinoIO /dev/ttyUSB0
loadusr -Wn arduinoIO-vcp pyvcp arduinoIO-vcp.xml

show pin arduinoIO-vcp

net din0 arduinoIO.digital-in-00 => arduinoIO-vcp.digital-in-00
net din1 arduinoIO.digital-in-01 => arduinoIO-vcp.digital-in-01
net din2 arduinoIO.digital-in-02 => arduinoIO-vcp.digital-in-02
net din3 arduinoIO.digital-in-03 => arduinoIO-vcp.digital-in-03
net din4 arduinoIO.digital-in-04 => arduinoIO-vcp.digital-in-04
net din5 arduinoIO.digital-in-05 => arduinoIO-vcp.digital-in-05

net dout0 arduinoIO.digital-out-01 <= arduinoIO-vcp.digital-out-01
net dout1 arduinoIO.digital-out-02 <= arduinoIO-vcp.digital-out-02
net dout2 arduinoIO.digital-out-03 <= arduinoIO-vcp.digital-out-03

net ain0 arduinoIO.analog-in-00 => arduinoIO-vcp.analog-in-00 arduinoIO-vcp.analog-in-00b
net ain1 arduinoIO.analog-in-01 => arduinoIO-vcp.analog-in-01 arduinoIO-vcp.analog-in-01b
net ain2 arduinoIO.analog-in-02 => arduinoIO-vcp.analog-in-02 arduinoIO-vcp.analog-in-02b
net ain3 arduinoIO.analog-in-03 => arduinoIO-vcp.analog-in-03 arduinoIO-vcp.analog-in-03b
net ain4 arduinoIO.analog-in-04 => arduinoIO-vcp.analog-in-04 arduinoIO-vcp.analog-in-04b
net ain5 arduinoIO.analog-in-05 => arduinoIO-vcp.analog-in-05 arduinoIO-vcp.analog-in-05b

net aout0 arduinoIO.analog-out-00 => arduinoIO-vcp.analog-out-00-f
net aout1 arduinoIO.analog-out-01 => arduinoIO-vcp.analog-out-01-f
net aout2 arduinoIO.analog-out-02 => arduinoIO-vcp.analog-out-02-f

waitusr arduinoIO-vcp
